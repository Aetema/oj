<br>
<div class="container">
  <ul class="nav nav-inline">
    <li class="nav-item">
      <a class="nav-link" href="/contest?cid={{.ContestID}}">Problems</a>
    </li>
    <li class="nav-item">
      <a class="nav-link" href="/contest/status?cid={{.ContestID}}">Status</a>
    </li>
    <li class="nav-item">
      <a class="nav-link" href="/contest/board?cid={{.ContestID}}">Board</a>
    </li>
  </ul>
  <hr>
</div>
  <table class="table table-bordered">
    <thead>
      <tr>
        <th>#</th>
        <th>user</th>
        <th>Total</th>
        <th>Time</th>
        {{range .Problems}}
        <th>{{i2c .}}</th>
        {{end}}
      </tr>
    </thead>
    <tbody>
      {{range $i,$v := .Users}}
      <tr>
        <th scope="row">{{inc $i}}</th>
        <td>{{$v.Username}}</td>
        <td>{{$v.ContestTotalAced}}</td>
        <td>{{$v.ContestTotalTime}}</td>
        {{range $j,$u:= .ContestWrongTimes}}
          <td {{if $v.FirstBlood[$j] }}bgcolor="seagreen"{{else}}{{if isAccepeted (i2c $j) $v.ContestAcedProblems}}bgcolor="LightGreen"{{else}} {{if haveWrong $u}}bgcolor="Tomato"{{end}}{{end}}{{end}}>{{if isAccepeted (i2c $j) $v.ContestAcedProblems}}{{getx $v.ContestAcedTime $j}}{{else}}-{{end}}/{{$u}}</td>
        {{end}}
      </tr>
      {{end}}
    </tbody>
  </table>
